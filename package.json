{
  "name": "battleship",
  "version": "1.0.0",
  "description": "An easy to use engine to make your own Battleship Game",
  "main": "index.js",
  "scripts": {
    "build": "npm run rollup && npm run minify && ncp dist/bge.min.js console/bge.min.js",
