  "license": "ISC",
  "devDependencies": {
    "clean-jsdoc-theme": "^3.3.4",
    "jsdoc": "^3.6.10",
    "ncp": "^2.0.0",
    "rollup": "^2.74.1",
    "terser": "^5.13.1"
  }
}
